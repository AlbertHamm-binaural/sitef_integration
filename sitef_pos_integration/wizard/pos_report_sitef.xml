<?xml version="1.0" encoding="utf-8"?>
<odoo>
        <record id="view_pos_sitef_wizard" model="ir.ui.view">
            <field name="name">pos.report.sitef.form</field>
            <field name="model">pos.report.sitef</field>
            <field name="arch" type="xml">
                <form string="Reportes por caja Sitef">
                    <group colspan="2">
                        <field name="start_date"/>
                        <field name="end_date"/>
                        <field name="type_report"/>
                    </group>
                    <footer>
                        <button name="generate_report" string="Print" type="object" class="btn-primary" data-hotkey="q"/>
                        <button string="Cancel" class="btn-secondary" special="cancel" data-hotkey="x" />
                    </footer>
                </form>
            </field>
        </record>

</odoo>